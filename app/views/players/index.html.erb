<% @players.each do |player| %>
  <section class="player">
    <div class="player-data">
      <img class="player-image">
        <%= image_tag player.picture %>
      <div class="player-header">
        <h2><%= player.name %></h2>
        <div class="num-positon">
          <p><%= player.number %></p>
          <p><%= player.position %></p>
        </div>
      </div>
    </div>
  </section>
<% end %>